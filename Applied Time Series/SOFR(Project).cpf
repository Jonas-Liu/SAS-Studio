ls\";\nproc http url = \"&URL_content\" method = \"get\" out = tmp;\nrun;\n\nproc import datafile = tmp dbms = excel replace out = _sofr;\nrun;\n\ndata sofr(keep = Date Rate);\n  set _sofr(firstobs = 4 \n            rename = ('Repo Rates'n = _Date 'Repo Rates2'n = _Rate)) \n            end = last nobs = nobs;\n  format Date date9.;\n  Date = input(_Date, YYMMDD10.);\n  Rate = input(_Rate, best12.);\n  if _n_ le nobs-10 then output;\nrun;","snippet":"","resource":false},"properties":{"left":"50","top":"140","width":"100","height":"60","region":"output","fillcolor":"#E0E6F1","linecolor":"#6882a3","tooltip":"Program 1","portsonly":true,"key":"control","visible":true}},{"version":2,"type":"code","id":"2ba1142c-e0c6-41ca-8551-cbbcedd47a30","name":"","label":"Sampling","description":"","created":1603995190213,"modified":1603995550241,"notes":"","parameters":{"server":"","target":"com.sas.ep.sascoder.execution.producers.VPP","action":"runSASCode","priority":"Reserved","code":"\n/* Set the proportion of the test set */\n%let ratio = 0.2;\n\n/* Train-Test Split */\ndata train test;\n  set sofr nobs = nobs;\n  if _n_ lt &ratio*nobs then output test;\n  else output train;\nrun;\n  \n  ","snippet":"","resource":false,"dependencies":["4a1eb17e-6a8d-4d1c-8c04-d979b2329089"]},"properties":{"left":"240","top":"140","width":"100","height":"60","region":"output","fillcolor":"#E0E6F1","linecolor":"#6882a3","tooltip":"Program","portsonly":true,"key":"control","visible":true}},{"version":2,"type":"code","id":"79f2d7dd-5c7e-4331-8dc3-d3b00cd6afb9","name":"","label":"Trend and Correlation Analysis","description":"","created":1603994560541,"modified":1603995628294,"notes":"","parameters":{"server":"","target":"com.sas.ep.sascoder.execution.producers.VPP","action":"runSASCode","priority":"Reserved","code":"\n/* Set the working dataset name */\n%let n_data = train;\n\ntitle \"Diagnostics for SOFR\";\nproc arima data = &n_data;\n  title2 \"Original Data\";\n  identify var = Rate; run;\nquit;\